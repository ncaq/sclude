{
  "name": "sclude",
  "version": "0.0.0",
  "author": "ncaq",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/(?!flycheck)*.+(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "jest": "23.4.2",
    "ts-jest": "23.1.3",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.14.0",
    "typescript": "3.0.1"
  },
  "dependencies": {
    "typescript-optional": "1.8.0"
  }
}
