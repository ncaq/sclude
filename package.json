    "array",
    "function",
    "haskell",
    "map",
    "object",
    "prelude",
    "standard",
    "string",
    "typescript"
  ],
  "homepage": "https://github.com/ncaq/sclude",
  "bugs": "https://github.com/ncaq/sclude/issues",
  "license": "MIT",
  "author": {
    "name": "ncaq",
    "email": "ncaq@ncaq.net",
    "url": "https://www.ncaq.net/"
  },
  "main": "dist/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ncaq/sclude.git"
  },
  "scripts": {
    "build": "tsc",
    "fix": "prettier --write **/*.ts --list-different && tslint --fix --project .",
